



case class ZucchiniSlice()
















































































































case class Zucchini(length: Double){
  def slice(): AQuantityOf[ZucchiniSlice] = List(new ZucchiniSlice())
}







type AQuantityOf[A] = List[A]



def sliceZucchini(z: Zucchini): AQuantityOf[ZucchiniSlice] = {
   z.slice().take(5)
}







